era:document a foaf:Document ;
    foaf:primaryTopic era: ;
    dct:hasFormat era: .

era: a owl:Ontology ;
    dct:title "ERA vocabulary"@en ;
    rdfs:label "ERA vocabulary"@en ;
    dct:issued "2020-08-24"^^xsd:date ;
    dct:modified "2020-08-25"^^xsd:date ;
    cc:license <https://creativecommons.org/licenses/by/4.0/> ;
    dct:creator <https://julianrojas.org#me> ;
    rdfs:comment "Vocabulary defined by the European Union Agency for Railways to describe the concepts and relationships related to the European railway infrastructure and the vehicles authorized to operate over it."@en ;
    owl:versionInfo "v0.3.8" .
    #owl:imports wgs:, geosparql:, geonames:.

#################################################################
#
#    Classes
#
#################################################################

######## Abstraction Layer #########

era:MicroNode a owl:Class, rdfs:Class ;
    rdfs:label "Micro Node"@en ;
    rdfs:comment "An abstraction of an operational point, used to provide a simple topological description of the railway infrastructure."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:MicroLink a owl:Class, rdfs:Class ;
    rdfs:label "Micro Link"@en ;
    rdfs:comment "An abstraction of a track that represent a physical rail connection between two different micro nodes."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:NodePort a owl:Class, rdfs:Class ;
    rdfs:subClassOf geosparql:Geometry, wgs:Point;
    rdfs:label "Node Port"@en ;
    rdfs:comment "A port to enter or exit a micro node. Represents the connection between a micro link and a micro node."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:InternalNodeLink a owl:Class, rdfs:Class ;
    rdfs:label "Internal Node Link"@en ;
    rdfs:comment "A specific track abstraction usee to represent a micro link switch that is possible inside a micro node."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .


######## Implementation Layer #########

era:OperationalPoint a owl:Class, rdfs:Class ;
    rdfs:label "Operational Point"@en ;
    rdfs:comment "An operational point (OP) means any location for train service operations, where train services may begin and end or change route, and where passenger or freight services may be provided; operational point means also any location at boundaries between Member States or infrastructure managers."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:subClassOf geosparql:Feature, wgs:SpatialThing;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:Track a owl:Class, rdfs:Class ;
    rdfs:label "Track"@en ;
    rdfs:comment "A running track that is used for train service movements."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:Tunnel a owl:Class, rdfs:Class ;
    rdfs:label "Tunnel"@en ;
    rdfs:comment "A railway tunnel is an excavation or a construction around the track provided to allow the railway to pass for example higher land, buildings or water."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:VehicleType a owl:Class, rdfs:Class ;
    rdfs:label "Vehicle Type"@en ;
    rdfs:comment "A vehicle type that has been authorized to operate on the EU railway infrastructure."@en ;
    rdfs:isDefinedBy era: ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

#################################################################
#
#    Object Properties
#
#################################################################

era:hasImplementation a owl:ObjectProperty, rdf:Property ;
    owl:inverseOf era:hasAbstraction ;
    rdfs:label "has implementation"@en ;
    rdfs:comment "Property that links a topological abstraction entity with its physical representation."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain [ a owl:Class ;
                  owl:unionOf ( era:MicroNode era:MicroLink )
                ] ;
    rdfs:range  [ a owl:Class ;
                  owl:unionOf ( era:OperationalPoint era:Track )
                ] ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:hasAbstraction a owl:ObjectProperty, rdf:Property ;
    owl:inverseOf era:hasImplementation ;
    rdfs:label "has abstraction"@en ;
    rdfs:comment "Property that links a physical entity with its topological abstraction representation."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain [ a owl:Class ;
                  owl:unionOf ( era:OperationalPoint era:Track )
                ] ;
    rdfs:range  [ a owl:Class ;
                  owl:unionOf ( era:MicroNode era:MicroLink )
                ] ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:opType a owl:ObjectProperty, rdf:Property ;
    rdfs:label "operational point type"@en ;
    rdfs:comment "Indicates the type on an operational point."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:OperationalPoint ;
    rdfs:range era-op-types:OperationalPointType ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:startPort a owl:ObjectProperty, rdf:Property ;
    rdfs:label "start port"@en ;
    rdfs:comment "Reference to the starting operational point's node port of a micro link or an internal node link."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain [ a owl:Class ;
                  owl:unionOf ( era:MicroLink era:InternalNodeLink )
                ] ;
    rdfs:range era:NodePort ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:endPort a owl:ObjectProperty, rdf:Property ;
    rdfs:label "end port"@en ;
    rdfs:comment "Reference to the ending operational point's node port of a micro link or an internal node link."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain [ a owl:Class ;
                  owl:unionOf ( era:MicroLink era:InternalNodeLink )
                ] ;
    rdfs:range era:NodePort ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:belongsToNode a owl:ObjectProperty, rdf:Property ;
    rdfs:label "belongs to node"@en ;
    rdfs:comment "Indicates the micro node to which a node port belongs to."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:NodePort ;
    rdfs:range era:MicroNode ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:startLocation a owl:ObjectProperty, rdf:Property ;
    rdfs:subPropertyOf geosparql:hasGeometry, wgs:location ;
    rdfs:label "start location"@en ;
    rdfs:comment "References the geolocation of the start of a tunnel."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:Tunnel ;
    rdfs:range [ a owl:Class ;
                  owl:unionOf ( geosparql:Geometry wgs:Point )
               ] ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:endLocation a owl:ObjectProperty, rdf:Property ;
    rdfs:subPropertyOf geosparql:hasGeometry, wgs:location ;
    rdfs:label "end location"@en ;
    rdfs:comment "References the geolocation of the end of a tunnel."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:Tunnel ;
    rdfs:range [ a owl:Class ;
                  owl:unionOf ( geosparql:Geometry wgs:Point )
               ] ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:hasTrack a owl:ObjectProperty, rdf:Property ;
    rdfs:label "has track"@en ;
    rdfs:comment "References a rail track that goes through a tunnel."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:Tunnel ;
    rdfs:range era:Track ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:passesThroughTunnel a owl:ObjectProperty, rdf:Property ;
    rdfs:label "passes through tunnel"@en ;
    rdfs:comment "References a tunnel that a track goes through."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:Track ;
    rdfs:range era:Tunnel ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:category a owl:ObjectProperty, rdf:Property ;
    rdfs:label "category"@en ;
    rdfs:comment "Vehicle category."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:VehicleType ;
    rdfs:range era-vehicles:Category ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:subCategory a owl:ObjectProperty, rdf:Property ;
    rdfs:label "subcategory"@en ;
    rdfs:comment "Vehicle subcategory."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:VehicleType ;
    rdfs:range era-vehicles:SubCategory ;
    dct:created "2020-07-29"^^xsd:date ;
    dct:modified "2020-07-29"^^xsd:date ;
    vs:term_status "stable" .

era:manufacturer a owl:ObjectProperty, rdf:Property ;
    rdfs:label "manufacturer"@en ;
    rdfs:comment "Vehicle manufacturer company."@en ;
    rdfs:isDefinedBy era: ;
    rdfs:domain era:VehicleType ;
    rdfs:range era-manufacturers:Manufacturer ;
